extends ../layout

block variables
  - var activeChapter='superadmin'
  - var activeSection='vjtelevision'


block inner-content
  include ../menu_user
  br
  .well
    form.form-inline(method="post")
      h3.form-group(style="margin:0 10px 0 0;")=__("Auto generate")
      .form-group
        select(name="month").form-control
          option=__("Month")
          option(value=1)=1
          option(value=2)=2
          option(value=3)=3
          option(value=4)=4
          option(value=5)=5
          option(value=6)=6
          option(value=7)=7
          option(value=8)=8
          option(value=9)=9
          option(value=10)=10
          option(value=11)=11
          option(value=12)=12
        select(name="year").form-control
          option=__("Year")
          - var year = new Date().getFullYear()-1;
          - for (var a=0;a<6;a++)
            option(value=year+a)=year+a
        input(type="submit", value=__("GENERATE")).btn.btn-primary

  - if (str)
    div!=str
    code!=update
